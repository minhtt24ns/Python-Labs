    "# 2D-> 5D\n",
    "arr_3 =np.expand_dims(arr_2 ,axis=(0,2,4))\n",
    "\n",
    "print(\"array 1:\",arr_1 , arr_1.shape)\n",
    "print(\"array 2:\",arr_2 , arr_2.shape)\n",
    "print(\"array 3:\\n\",arr_3, arr_3.shape)"
